{
  "name": "consultly",
  "version": "0.0.1",
  "description": "Consultly",
  "author": "Suresh Ojha",
  "engines": {
    "node": ">=22.0.0"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "cross-env NODE_ENV=development nest start",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "swc": "cross-env NODE_ENV=development nest start -b swc --type-check -w",
    "prod": "cross-env NODE_ENV=production node dist/main",
    "build": "nest build",
    "clean": "rm -rf dist",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=development jest --passWithNoTests",
    "test:cov": "cross-env NODE_ENV=development jest --coverage",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "doc": "npx @compodoc/compodoc -p tsconfig.json -s --port 3001 --watch -d ./documentation",
    "migrate": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/migrate.ts",
    "prepare": "husky",
    "generate:module": "ts-node scripts/generate-module.ts"
  },
  "lint-staged": {
    "{src,apps,libs,test}/**/*.ts": ["eslint --fix", "prettier --write"]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.965.0",
    "@aws-sdk/lib-storage": "^3.965.0",
    "@bull-board/api": "^6.16.2",
    "@bull-board/express": "^6.16.2",
    "@bull-board/nestjs": "^6.16.2",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^11.1.11",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/qrcode": "^1.5.6",
    "argon2": "^0.44.0",
    "bullmq": "^5.66.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.1",
    "ejs": "^3.1.10",
    "email-templates": "^13.0.1",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.9.0",
    "joi": "^18.0.2",
    "lodash": "^4.17.23",
    "mongoose": "^9.0.1",
    "multer": "^2.0.2",
    "nest-winston": "^1.10.2",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "qrcode": "^1.5.4",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.3",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.7",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/compression": "^1.8.1",
    "@types/ejs": "^3.1.5",
    "@types/email-templates": "^10.0.4",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.23",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^7.0.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.2",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "overrides": {
      "glob": "^10.0.0"
    }
  }
}
